@model List<Webshop.Models.Product>


<h1>Winkelwagen Overzicht</h1>
@if(Model.Count != 0){

    @foreach(var order_product in Model){
        @Html.Partial("_BasketRow", order_product)
    }

        <table>
            <tr>
                <td>@Html.ActionLink("Order", "Order", "Basket")</td>
            </tr>
        </table>
} else {
   @Html.Partial("_OrderRow")
    
}